@model IEnumerable<ChildhoodVaccination.Models.Child>
<html>
<head>
    <title>Index page</title>
</head>
<body>
    <table>
        <tr>
            <th>ID</th>
            <th>First name</th>
            <th>Last name</th>
            <th>DOB</th>
            <th>Phone</th>
            <th>Created</th>
        </tr>
        @foreach (var child in Model)
        {
        <tr>
            <td>@child.ChildID</td>
            <td>@child.FirstName</td>
            <td>@child.LastName</td>
            <td>@child.DateOfBirth</td>
            <td>@child.Phone</td>
            <td>@child.Created</td>
        </tr>
        }
    </table>
</body>
</html>